			a {
				color: #EF5353;
				text-decoration: none;
			}
			header{
				margin-bottom:9em;
			}
		        header .menu {
					position:fixed;
					width:12em;
					height:12em;
					left:50%;
					top:-6em;
					border:solid 2px #444;
					border-radius:6em;
					background-color:#fff;
					margin-left:-6em;
					z-index:99;
				}
					header .menu p {
						position:relative;
						bottom:-6em;
						text-align: center;
						font-weight: bold;
						margin: 0 auto;
					}
						header .menu p a{
							text-transform: uppercase;
							letter-spacing: 2px;
							font-size: 20px;
							font-family: "Montserrat", arial, sans-serif;
							color:#444;
							text-decoration:none;
						}
				header .menu:hover {
					background-color:#444;
				}
				header .menu:hover a {
					color:#fff;
				}
        header .navWrapper {
            width: 52em;
            height: 52em;
            overflow: hidden;
            border-radius: 50%;
            margin-left: -26em;
            position: fixed;
            left: 50%;
            top: -27em;
            transform: scale(.4);
			z-index:98;
        }
            header .navWrapper ul {
                list-style-type: none;
                position: relative;
				margin:0;
            }
                header .navWrapper ul li {
                    position: absolute;
                    left: 50%;
                    top: 50%;
                    display: block;
                    background-color: #d9534f;
                    font-size: 3em;
                    width: 10em;
                    height: 10em;
                    -ms-transform-origin: 100% 100%; /* IE 9 */
                    -webkit-transform-origin: 100% 100%; /* Chrome, Safari, Opera */
                    transform-origin: 100% 100%;
                    overflow: hidden;
                    margin-top: -1.3em;
                    margin-left: -10em;
                    cursor:pointer;
                }
                    header .navWrapper ul li:first-of-type {
                        transform: rotate(315deg) skew(45deg);
                    }
                    header .navWrapper ul li:nth-of-type(2) {
                        transform: rotate(270deg) skew(45deg);
                    }
                    header .navWrapper ul li:nth-of-type(3) {
                        transform: rotate(225deg) skew(45deg);
                    }
                    header .navWrapper ul li:nth-of-type(4) {
                        transform: rotate(180deg) skew(45deg);
                    }
                    header .navWrapper ul li a {
                        background-color: #d9534f;
                        display: block;
                        font-size: 1.18em;
                        height: 14.5em;
                        width: 14.5em;
                        position: absolute;
                        right: -7.25em;
                        border-radius: 50%;
                        text-decoration: none;
                        color: #fff;
                        padding-top: 2.2em;
						padding-right: .75em;
                        text-align: center;
                        -webkit-transform: skew(-45deg) rotate(-70deg) scale(1);
                        -ms-transform: skew(-45deg) rotate(-70deg) scale(1);
                        -moz-transform: skew(-45deg) rotate(-70deg) scale(1);
                        transform: skew(-45deg) rotate(-70deg) scale(1);
                        -webkit-backface-visibility: hidden;
                        -webkit-transition: opacity 0.3s, color 0.3s;
                        -moz-transition: opacity 0.3s, color 0.3s;
                        transition: opacity 0.3s, color 0.3s;
                    }
                    header .navWrapper ul li:hover a {
						background-color:#444;
                    }
						header .navWrapper ul li a i{
							-webkit-transform: skew(0deg) scale(1);
							-ms-transform: skew(0deg) scale(1);
							-moz-transform: skew(0deg) scale(1);
							transform: skew(0deg) scale(1);
						}
						header .navWrapper ul li:first-of-type a i {
							transform: rotate(115deg);
						}
						header .navWrapper ul li:nth-of-type(2) a i  {
							transform: rotate(160deg);
						}
						header .navWrapper ul li:nth-of-type(3) a i  {
							transform:  rotate(-155deg);
						}
						header .navWrapper ul li:nth-of-type(4) a i  {
							transform:  rotate(-110deg);
						}
